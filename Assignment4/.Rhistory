library(mclust)
model <- Mclust(seeds, G=3)
plot(model, what="classification")
model$z[model$z >= 0.5] <- 1
model$z[model$z < 0.5] <- 0
classif <- data.frame(rep(1, dim(model$z)[1]))
classif[as.logical(model$z[,2]),] <- 2
classif[as.logical(model$z[,3]),] <- 3
(cross.table.mix <- table(classif$rep.1..dim.model.z..1.., variedad))
sum(diag(cross.table.mix))/sum(cross.table.mix)
1-sum(diag(cross.table.mix))/sum(cross.table.mix)
